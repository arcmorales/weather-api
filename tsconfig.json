{
  "include": ["commons", "tests/*"]
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "noImplicitOverride": true,
    "noFallthroughCasesInSwitch": true,
    "useUnknownInCatchVariables": false,
    "baseUrl": "."
  }
}
